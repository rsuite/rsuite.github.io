{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-1",
    "react",
    ["rsuite", { "style": true }]
  ],
  "plugins": [
    "lodash",
    "transform-proto-to-assign",
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-dev"
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
